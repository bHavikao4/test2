package test2;

public class P3ifanarraycontainsaspecificvalue {

	public static void main(String[] args) {
		int[] num = {1, 2, 3, 4, 5};
	    int find = 3;
	    boolean found = false;

	    for (int n : num) {
	      if (n == find) {
	        found = true;
	        break;
	      }
	    }
	    
	    if(found)
	      System.out.println(find + " is found.");
	    else
	      System.out.println(find + " is not found.");

	}

}
