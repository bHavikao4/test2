package test2;
public class P3theaveragevalueofarray {

	public static void main(String[] args) {
		int a[] = { 1,2,3,4,5 };
        int b = a.length;
        int sum = 0;
        for (int i = 0; i < a.length; i++) {
            sum += a[i];
        }
 
        double average = sum / b;
         
        System.out.println("Average: "+average);
 
    }
 
}
